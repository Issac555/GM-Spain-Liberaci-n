forward ShowHalloWeen(playerid);
public ShowHalloWeen(playerid)
{
    new dialog[250], string[250];

    if(event_Hallowen) strcat(dialog, "Estado del evento\tActivado\n");
    else strcat(dialog, "Estado del evento\tDesactivado\n");

    format(string, sizeof(string), "Calabazas no encontradas\t%d\n", GetCountFinded());
    strcat(dialog, string);

    format(string, sizeof(string), "Calabazas totales\t%d", sizeof(Halloween));
    strcat(dialog, string);

    ShowPlayerDialog(playerid, DIALOG_MAIN_HALLO, DIALOG_STYLE_LIST, "Evento - Halloween", dialog, "Aceptar", "Cancelar");
    return true;

}

CMD:ahalloween(playerid, params[])
{
    #pragma unused params
    ShowHalloWeen(playerid);
    return true;
}


forward GetCountFinded();
public GetCountFinded()
{
    new count = 0;
    for(new i = 0; i < sizeof(Halloween); i++)
    {
        if(Halloween[i][hallo_finded] == false)
        {
            count++;
        }
    }
    return count;
}

hook OnDialogResponse(playerid, dialogid, response, listitem, inputtext[])
{
    switch(dialogid)
    {
        case DIALOG_MAIN_HALLO:
        {
            if(!response) return false;
            if(listitem == 0)
            {
                if(event_Hallowen == false)
                {
                    SendClientMessageToAll(0xfc6f03FF, "[Evento] El evento de las calabazas fue iniciado por un administrador");
                    SendClientMessageToAll(0xfc6f03FF, "-- Encuentra las calabazas por todo el mapa para obtener premios");
                    event_Hallowen = true;
                    CreateEventCalabazas();
                    return true;
                }
                else 
                {
                    SendClientMessageToAll(0xfc6f03FF, "[Evento] El evento de las calabazas fue finalizado");
                    event_Hallowen = false;
                    DestroyEventCalabazas();
                    return true;
                }
            }
        }
    }
    return Y_HOOKS_CONTINUE_RETURN_1;
}


forward CreateEventCalabazas();
public CreateEventCalabazas()
{
    for(new i = 0; i < sizeof(Halloween); i++)
    {
        Halloween[i][hallo_finded] = false;
        if(!IsValidDynamic3DTextLabel(Halloween[i][hallo_label]))
        {
            Halloween[i][hallo_label] =  CreateDynamic3DTextLabel("{fa5914}Presiona [ H ] para tomar la calabaza", -1, Halloween[i][hallo_x], Halloween[i][hallo_y], Halloween[i][hallo_z], 1.5, INVALID_PLAYER_ID, INVALID_VEHICLE_ID, 0, -1, -1);
        }
        Halloween[i][hallo_object] = CreateDynamicObject(19320, Halloween[i][hallo_x], Halloween[i][hallo_y], Halloween[i][hallo_z], 0.0, 0.0, 0.0, -1,  -1, -1);
    }   
    return true;
}

forward DestroyEventCalabazas();
public DestroyEventCalabazas()
{
    for(new i = 0; i < sizeof(Halloween); i++)
    {
        if(IsValidDynamic3DTextLabel(Halloween[i][hallo_label])) DestroyDynamic3DTextLabel(Halloween[i][hallo_label]);
        if(IsValidDynamicObject(Halloween[i][hallo_object])) DestroyDynamicObject(Halloween[i][hallo_object]);
    }
    return true;
}

hook OnPlayerKeyStateChange(playerid, newkeys, oldkeys)
{
    if(newkeys &  KEY_CTRL_BACK)
    {
        if(event_Hallowen)
        {
            for(new i = 0; i < sizeof(Halloween); i++)
            {
                if(IsPlayerInRangeOfPoint(playerid, 1.5, Halloween[i][hallo_x], Halloween[i][hallo_y], Halloween[i][hallo_z]))
                {
                    if(Halloween[i][hallo_finded]) return false;
                    switch(Halloween[i][hallo_type])
                    {
                        case TYPE_CRACK:
                        {
                            new crack = random(20 - 5) + 5;
                            PI[playerid][pi_CRACK] += crack;
                            SendClientMessageEx(playerid, -1, "Encontraste %d de crack", crack);
                        }
                        case TYPE_MEDIS:
                        {
                            new medicine = random(20 - 5) + 5;
                            PI[playerid][pi_MEDICINE] += medicine;
                            SendClientMessageEx(playerid, -1, "Encontraste %d de medicina", medicine);
                        }
                        case TYPE_DINERO:
                        {
                            new dinerorand = random(6000 - 2000) + 2000;
                            GivePlayerCash(playerid, dinerorand, true, false);
                            SendClientMessageEx(playerid, -1, "Encontraste %s$ de dinero", number_format_thousand(dinerorand));
                        }

                    }

                    Halloween[i][hallo_finded] = true;
                    if(IsValidDynamic3DTextLabel(Halloween[i][hallo_label])) DestroyDynamic3DTextLabel(Halloween[i][hallo_label]);
                    if(IsValidDynamicObject(Halloween[i][hallo_object])) DestroyDynamicObject(Halloween[i][hallo_object]);
                    Halloween[i][hallo_label] = Text3D: -1;
                    Halloween[i][hallo_object] = INVALID_STREAMER_ID;
                    break;
                }
            }
            return true;
        }
    }
    return Y_HOOKS_CONTINUE_RETURN_1;
}

